[submodule ".registry"]
	path = .registry
	url = https://github.com/r-universe-org/cran-to-git
	shallow = true
	branch = HEAD
[submodule "delayed"]
	path = delayed
	url = https://github.com/tlverse/delayed
	shallow = true
	registered = true
[submodule "origami"]
	path = origami
	url = https://github.com/tlverse/origami
	shallow = true
	registered = true
